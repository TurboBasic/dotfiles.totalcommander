  cmd=cd "%Public%\Desktop"
menu=Start Menu
  cmd=cd "%$COMMON_STARTMENU%"
menu=Autoload
  cmd=cd "%$COMMON_STARTUP%"
menu=-
menu=Application Data
  cmd=cd "%ProgramData%"
