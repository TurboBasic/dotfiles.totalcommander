menu=Autoload
 cmd=cd "%$STARTUP%"
menu=TaskBar Pinned Shortcuts
 cmd=cd "%$APPDATA%\Microsoft\Internet Explorer\Quick Launch\User Pinned\TaskBar"
menu=-
menu=SendTo
  cmd=cd "%UserProfile%\SendTo"
menu=Quick Launch
  cmd=cd "%Appdata%\Microsoft\Internet Explorer\Quick Launch"
menu=Recent
  cmd=cd "%UserProfile%\Recent"
menu=-
menu=Local Application Data
cmd=cd "%LocalAppdata%"
